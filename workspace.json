{
  "version": 2,
  "projects": {
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/core"],
          "options": {
            "jestConfig": "packages/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/core",
            "tsConfig": "packages/core/tsconfig.lib.json",
            "packageJson": "packages/core/package.json",
            "main": "packages/core/src/index.ts",
            "updateBuildableProjectDepsInPackageJson": true,
            "assets": [
              "packages/core/*.md",
              {
                "input": "./packages/core/src",
                "glob": "**/*.!(ts|spec.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/core",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/core",
                "glob": "executors.json",
                "output": "."
              },
              "LICENSE",
              "CHANGELOG.md",
              "README.md"
            ]
          }
        }
      }
    },
    "core-e2e": {
      "projectType": "application",
      "root": "e2e\\core-e2e",
      "sourceRoot": "e2e\\core-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "core:build",
            "npmPackageName": "@nx-dotnet/core",
            "pluginOutputPath": "dist\\packages\\core",
            "jestConfig": "e2e/core-e2e/jest.config.js"
          }
        }
      }
    },
    "typescript": {
      "root": "packages/typescript",
      "sourceRoot": "packages/typescript/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/typescript/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/typescript"],
          "options": {
            "jestConfig": "packages/typescript/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/typescript",
            "tsConfig": "packages/typescript/tsconfig.lib.json",
            "packageJson": "packages/typescript/package.json",
            "main": "packages/typescript/src/index.ts",
            "updateBuildableProjectDepsInPackageJson": true,
            "assets": [
              "packages/typescript/*.md",
              {
                "input": "./packages/typescript/src",
                "glob": "**/*.!(ts|spec.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/typescript",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/typescript",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "typescript-e2e": {
      "projectType": "application",
      "root": "e2e\\typescript-e2e",
      "sourceRoot": "e2e\\typescript-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "typescript:build",
            "npmPackageName": "@nx-dotnet/typescript",
            "pluginOutputPath": "dist/packages/typescript",
            "jestConfig": "e2e/typescript-e2e/jest.config.js"
          }
        }
      }
    },
    "utils": {
      "root": "packages/utils",
      "sourceRoot": "packages/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/utils"],
          "options": {
            "jestConfig": "packages/utils/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/utils",
            "tsConfig": "packages/utils/tsconfig.lib.json",
            "packageJson": "packages/utils/package.json",
            "updateBuildableProjectDepsInPackageJson": true,
            "main": "packages/utils/src/index.ts",
            "assets": ["packages/utils/*.md"]
          }
        }
      }
    },
    "dotnet": {
      "root": "packages/dotnet",
      "sourceRoot": "packages/dotnet/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/dotnet/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/dotnet"],
          "options": {
            "jestConfig": "packages/dotnet/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/dotnet",
            "tsConfig": "packages/dotnet/tsconfig.lib.json",
            "packageJson": "packages/dotnet/package.json",
            "main": "packages/dotnet/src/index.ts",
            "updateBuildableProjectDepsInPackageJson": true,
            "assets": ["packages/dotnet/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
